<!-- Main Menu Section -->
<section class="menu">
	<nav class="navbar navigation">
		<div class="container">
			<div class="navbar-header">
				<button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar"
					aria-expanded="false" aria-controls="navbar">
					<span class="sr-only">Toggle navigation</span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
				</button>

			</div><!-- / .navbar-header -->

			<!-- Navbar Links -->
			<div id="navbar" class="navbar-collapse collapse text-center">
				<ul class="nav navbar-nav">
                    <% category.forEach(function(cate0){ %>
                        <% if(cate0.cParentId==0){ %>
                            <% if(cate0.cAlone == true){ %>
                                <li class="dropdown">
                                    <a href="<%= process.env.DOMAIN_NAME%>/<%= cate0.cUrl %>"><%= cate0.cName %></a>
                                </li>
                            <% } else { %>
                                <li class="dropdown dropdown-slide">
                                    <a href="<%= process.env.DOMAIN_NAME%>/<%= cate0.cUrl %>" class="dropdown-toggle" data-toggle="dropdown" data-hover="dropdown" data-delay="350" role="button" aria-haspopup="true" aria-expanded="false"><%= cate0.cName %> <span class="tf-ion-ios-arrow-down"></span></a>
                                    <div class="dropdown-menu">
                                        <div class="row">
                                            <div class="col-lg-12 col-md-12 mb-sm-12">
                                                <ul>
                                                    <% category.forEach(function(cate1){ %>
                                                        <% if(cate0._id == cate1.cParentId){ %>
                                                            <li><a href="<%= process.env.DOMAIN_NAME %>/<%= cate0.cUrl %>/<%= cate1.cUrl %>"><%= cate1.cName %></a></li>
                                                        <% } %>
                                                    <% }) %>
                                                </ul>
                                            </div>
                                        </div>
                                    </div>
                                </li>
                            <% } %>
                        <% } %>
                    <% }) %>

				</ul><!-- / .nav .navbar-nav -->

			</div>
			<!--/.navbar-collapse -->
		</div><!-- / .container -->
	</nav>
</section>